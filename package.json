{
	"name": "cloudflare-worker-boilerplate",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"bun": ">=1.0.0"
	},
	"scripts": {
		"preinstall": "case \"$npm_config_user_agent\" in bun*) ;; *) echo 'Error: This project requires Bun. Install: https://bun.sh' >&2; exit 1;; esac",
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "bun test",
		"cf-typegen": "wrangler types",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint .",
		"lint:format": "biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@types/bun": "^1.3.8",
		"@types/node": "^25.2.1",
		"typescript": "^5.5.2",
		"wrangler": "^4.63.0"
	}
}
